         <!--FORM-->

         %h3 Form

         <!--Basic FORM-->

         %h4 Basic Form
         %form.form.well
            .form-group
               %label(for="name1") This is label 1
               %input.form-control(type="type" id="name1" placeholder="Placeholder1")
            .form-group
               %label(for="name2") This is label 2
               %input.form-control.input-lg(type="type" id="name2" placeholder="This is large input")
            .form-group
               %label(for="name3") This is label 3
               %input.form-control.input-sm(type="type" id="name3" placeholder="This is small input")
            .form-group
               %label This is textarea
               %textarea.form-control(placeholder="Placeholder3")
            .form-group
               %label(for="name4") This is select
               %select.form-control
                  %option 1
                  %option 2
                  %option 3
                  %option 4
            .checkbox
               %label
                  %input(type="checkbox")
                  Check me
            %button.btn.btn-default Submit

         <!--FORM inline-->

         %h4 Inline Form
         %form.form-inline.well
            .form-group
               %label.sr-only(for="name1") This is label 1
               %input.form-control(type="type" id="name1" placeholder="Placeholder1")
            .form-group
               %label.sr-only(for="name2") This is label 2
               %input.form-control(type="type" id="name2" placeholder="Placeholder2")
            .checkbox
               %label
                  %input(type="checkbox")
                  Check me
            %button.btn.btn-default Submit

         <!--Horizontal FORM-->

         %h4 Horizontal Form
         %form.form-horizontal.well
            .form-group
               %label.col-sm-3.control-label(for="name1") This is label 1
               .col-sm-9
                  %input.form-control(type="type" id="name1" placeholder="Placeholder1")
            .form-group
               %label.col-sm-3.control-label(for="name2") This is label 2
               .col-sm-9
                  %input.form-control(type="type" id="name2" placeholder="Placeholder2")
            .form-group
               %label.col-sm-3.control-label This is textarea
               .col-sm-9
                  %textarea.form-control(placeholder="Placeholder3")
            .form-group
               .col-sm-offset-3.col-sm-10
                  .checkbox
