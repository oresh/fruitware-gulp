         <!--NAV-->

         %h3 Menu

         <!--NAV HORIZONTAL-->

         %h4 Menu horizontal

         <!--one level NAV-->

         %h5 One level menu
         %ul.menu.menu-horizontal.menu--name.well
            %li.menu-item
               %a.menu-link(href="#") Menu item 1
            %li.menu-item
               %a.menu-link(href="#") Menu item 2
            %li.menu-item.active-trail
