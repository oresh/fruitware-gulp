               %a.menu-link(href="#")
                  %span.fa.fa-fire-extinguisher
                  Menu item 1 FAWESOME
            %li.menu-item
               %a.menu-link(href="#")
                  %span.glyphicon.glyphicon-envelope
                  Menu item 2 GLYPHICON
            %li.menu-item
               %a.menu-link(href="#") Menu item 3 No icon
            %li.menu-item.depth-1.active-trail
               %a.menu-link.active(href="#") Menu item 4 active

